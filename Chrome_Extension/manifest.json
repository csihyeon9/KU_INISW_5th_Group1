{
    "manifest_version": 3,
    "name": "Finï¿¦i$E",
    "version": "1.2",
    "permissions": [
        "sidePanel",
        "activeTab",
        "scripting",
        "storage"
    ],
    "web_accessible_resources": [{
        "resources": ["images/icon.png"],
        "matches": ["<all_urls>"]
    }],
    "action": {
        "default_icon": "images/icon.png"
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    }
}